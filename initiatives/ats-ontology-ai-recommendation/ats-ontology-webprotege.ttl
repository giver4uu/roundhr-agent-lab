@prefix : <http://round.pm/ats-ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:ATSRecruitingOntology rdf:type owl:Ontology ;
  rdfs:comment "Ontology derived from ATS ontology PM strategy (2025-12-03)"@en .

#################################################################
# Classes (Objects)
#################################################################
:Candidate rdf:type owl:Class ;
  rdfs:label "Candidate"@en ;
  rdfs:comment "Individual who applied to at least one job posting."@en .

:JobPosting rdf:type owl:Class ;
  rdfs:label "Job Posting"@en ;
  rdfs:comment "Open role to which candidates can apply."@en .

:Application rdf:type owl:Class ;
  rdfs:label "Application"@en ;
  rdfs:comment "Submission that connects a candidate to a job posting."@en .

:RecruitmentStage rdf:type owl:Class ;
  rdfs:label "Recruitment Stage"@en ;
  rdfs:comment "Defined stage within the hiring funnel."@en .

:StageTransition rdf:type owl:Class ;
  rdfs:label "Stage Transition"@en ;
  rdfs:comment "Event that captures movement between recruitment stages."@en .

:Interview rdf:type owl:Class ;
  rdfs:label "Interview"@en ;
  rdfs:comment "Scheduled interview event."@en .

:Evaluation rdf:type owl:Class ;
  rdfs:label "Evaluation"@en ;
  rdfs:comment "Assessment record linked to interviews and candidates."@en .

:AIRecommendation rdf:type owl:Class ;
  rdfs:label "AI Recommendation"@en ;
  rdfs:comment "Suggestion generated by AI with transparency, override, and learning loop metadata."@en .

:HiringManager rdf:type owl:Class ;
  rdfs:label "Hiring Manager"@en .

:Interviewer rdf:type owl:Class ;
  rdfs:label "Interviewer"@en .

:Recruiter rdf:type owl:Class ;
  rdfs:label "Recruiter"@en .

:Communication rdf:type owl:Class ;
  rdfs:label "Communication"@en ;
  rdfs:comment "Interaction record between recruiter and candidate."@en .

:EvaluationRubric rdf:type owl:Class ;
  rdfs:label "Evaluation Rubric"@en .

:Skill rdf:type owl:Class ;
  rdfs:label "Skill"@en .

:AssessmentTemplate rdf:type owl:Class ;
  rdfs:label "Assessment Template"@en .

:Task rdf:type owl:Class ;
  rdfs:label "Task"@en .

:Department rdf:type owl:Class ;
  rdfs:label "Department"@en .

:ApplicationMetrics rdf:type owl:Class ;
  rdfs:label "Application Metrics"@en ;
  rdfs:comment "Aggregated KPIs tied to an application."@en .

#################################################################
# Object Properties (Links)
#################################################################
:appliesTo rdf:type owl:ObjectProperty ;
  rdfs:label "applies to"@en ;
  rdfs:domain :Candidate ;
  rdfs:range :JobPosting ;
  owl:inverseOf :hasApplicant .

:hasApplicant rdf:type owl:ObjectProperty ;
  rdfs:domain :JobPosting ;
  rdfs:range :Candidate .

:creates rdf:type owl:ObjectProperty ;
  rdfs:label "creates"@en ;
  rdfs:domain :Application ;
  rdfs:range :Candidate ;
  owl:inverseOf :createdBy .

:createdBy rdf:type owl:ObjectProperty ;
  rdfs:domain :Candidate ;
  rdfs:range :Application .

:forPosition rdf:type owl:ObjectProperty ;
  rdfs:label "for position"@en ;
  rdfs:domain :Application ;
  rdfs:range :JobPosting ;
  owl:inverseOf :hasApplication .

:hasApplication rdf:type owl:ObjectProperty ;
  rdfs:domain :JobPosting ;
  rdfs:range :Application .

:schedules rdf:type owl:ObjectProperty ;
  rdfs:label "schedules"@en ;
  rdfs:domain :Interview ;
  rdfs:range :Application ;
  owl:inverseOf :hasInterview .

:hasInterview rdf:type owl:ObjectProperty ;
  rdfs:domain :Application ;
  rdfs:range :Interview .

:belongsToDepartment rdf:type owl:ObjectProperty ;
  rdfs:label "belongs to department"@en ;
  rdfs:domain :JobPosting ;
  rdfs:range :Department ;
  owl:inverseOf :ownsPosition .

:ownsPosition rdf:type owl:ObjectProperty ;
  rdfs:domain :Department ;
  rdfs:range :JobPosting .

:evaluates rdf:type owl:ObjectProperty ;
  rdfs:label "evaluates"@en ;
  rdfs:domain :Interviewer ;
  rdfs:range :Candidate ;
  owl:inverseOf :evaluatedBy .

:evaluatedBy rdf:type owl:ObjectProperty ;
  rdfs:domain :Candidate ;
  rdfs:range :Interviewer .

:recommends rdf:type owl:ObjectProperty ;
  rdfs:label "recommends"@en ;
  rdfs:domain :HiringManager ;
  rdfs:range :Candidate ;
  owl:inverseOf :recommendedBy .

:recommendedBy rdf:type owl:ObjectProperty ;
  rdfs:domain :Candidate ;
  rdfs:range :HiringManager .

:communicatesWith rdf:type owl:ObjectProperty ;
  rdfs:label "communicates with"@en ;
  rdfs:domain :Recruiter ;
  rdfs:range :Candidate ;
  owl:inverseOf :communicatedBy .

:communicatedBy rdf:type owl:ObjectProperty ;
  rdfs:domain :Candidate ;
  rdfs:range :Recruiter .

:hasCommunication rdf:type owl:ObjectProperty ;
  rdfs:domain :Communication ;
  rdfs:range :Candidate .

:progressesTo rdf:type owl:ObjectProperty ;
  rdfs:label "progresses to"@en ;
  rdfs:domain :StageTransition ;
  rdfs:range :RecruitmentStage ;
  owl:inverseOf :receivesFrom .

:receivesFrom rdf:type owl:ObjectProperty ;
  rdfs:domain :RecruitmentStage ;
  rdfs:range :StageTransition .

:transitionForApplication rdf:type owl:ObjectProperty ;
  rdfs:domain :StageTransition ;
  rdfs:range :Application .

:assigns rdf:type owl:ObjectProperty ;
  rdfs:label "assigns"@en ;
  rdfs:domain :Recruiter ;
  rdfs:range :Application ;
  owl:inverseOf :assignedTo .

:assignedTo rdf:type owl:ObjectProperty ;
  rdfs:domain :Application ;
  rdfs:range :Recruiter .

:hasSkill rdf:type owl:ObjectProperty ;
  rdfs:label "has skill"@en ;
  rdfs:domain :Candidate ;
  rdfs:range :Skill ;
  owl:inverseOf :skillOfCandidate .

:skillOfCandidate rdf:type owl:ObjectProperty ;
  rdfs:domain :Skill ;
  rdfs:range :Candidate .

:requiresSkill rdf:type owl:ObjectProperty ;
  rdfs:label "requires skill"@en ;
  rdfs:domain :JobPosting ;
  rdfs:range :Skill .

:usesRubric rdf:type owl:ObjectProperty ;
  rdfs:domain :Evaluation ;
  rdfs:range :EvaluationRubric .

:recommendsFor rdf:type owl:ObjectProperty ;
  rdfs:label "recommends for"@en ;
  rdfs:domain :AIRecommendation ;
  rdfs:range owl:Thing .

:aiRecommendationTarget rdf:type owl:ObjectProperty ;
  rdfs:domain :AIRecommendation ;
  rdfs:range :Application .

:aiRecommendationReason rdf:type owl:ObjectProperty ;
  rdfs:domain :AIRecommendation ;
  rdfs:range :StageTransition .

:interviewedBy rdf:type owl:ObjectProperty ;
  rdfs:label "interviewed by"@en ;
  rdfs:domain :Application ;
  rdfs:range :Interviewer .

:competesWith rdf:type owl:ObjectProperty ;
  rdfs:label "competes with"@en ;
  rdfs:domain :Candidate ;
  rdfs:range :Candidate ;
  rdf:type owl:SymmetricProperty .

:previouslyApplied rdf:type owl:ObjectProperty ;
  rdfs:label "previously applied"@en ;
  rdfs:domain :Candidate ;
  rdfs:range :Application .

:hasMetrics rdf:type owl:ObjectProperty ;
  rdfs:domain :Application ;
  rdfs:range :ApplicationMetrics .

#################################################################
# Data Properties (Key Attributes)
#################################################################
:candidateId rdf:type owl:DatatypeProperty ;
  rdfs:domain :Candidate ;
  rdfs:range xsd:string .

:fullName rdf:type owl:DatatypeProperty ;
  rdfs:domain :Candidate ;
  rdfs:range xsd:string .

:email rdf:type owl:DatatypeProperty ;
  rdfs:domain :Candidate ;
  rdfs:range xsd:string .

:phone rdf:type owl:DatatypeProperty ;
  rdfs:domain :Candidate ;
  rdfs:range xsd:string .

:appliedDate rdf:type owl:DatatypeProperty ;
  rdfs:domain :Application ;
  rdfs:range xsd:dateTime .

:currentStage rdf:type owl:DatatypeProperty ;
  rdfs:domain :Application ;
  rdfs:range xsd:string .

:currentStageEnteredAt rdf:type owl:DatatypeProperty ;
  rdfs:domain :Application ;
  rdfs:range xsd:dateTime .

:overallStatus rdf:type owl:DatatypeProperty ;
  rdfs:domain :Application ;
  rdfs:range xsd:string .

:stageName rdf:type owl:DatatypeProperty ;
  rdfs:domain :RecruitmentStage ;
  rdfs:range xsd:string .

:sequenceOrder rdf:type owl:DatatypeProperty ;
  rdfs:domain :RecruitmentStage ;
  rdfs:range xsd:int .

:averageDuration rdf:type owl:DatatypeProperty ;
  rdfs:domain :RecruitmentStage ;
  rdfs:range xsd:duration .

:transitionTimestamp rdf:type owl:DatatypeProperty ;
  rdfs:domain :StageTransition ;
  rdfs:range xsd:dateTime .

:durationInPreviousStage rdf:type owl:DatatypeProperty ;
  rdfs:domain :StageTransition ;
  rdfs:range xsd:decimal .

:triggeredBy rdf:type owl:DatatypeProperty ;
  rdfs:domain :StageTransition ;
  rdfs:range xsd:string .

:interviewType rdf:type owl:DatatypeProperty ;
  rdfs:domain :Interview ;
  rdfs:range xsd:string .

:scheduledDate rdf:type owl:DatatypeProperty ;
  rdfs:domain :Interview ;
  rdfs:range xsd:dateTime .

:evaluationScore rdf:type owl:DatatypeProperty ;
  rdfs:domain :Evaluation ;
  rdfs:range xsd:decimal .

:evaluationFeedback rdf:type owl:DatatypeProperty ;
  rdfs:domain :Evaluation ;
  rdfs:range xsd:string .

:confidenceLevel rdf:type owl:DatatypeProperty ;
  rdfs:domain :Evaluation ;
  rdfs:range xsd:decimal .

:recommendationType rdf:type owl:DatatypeProperty ;
  rdfs:domain :AIRecommendation ;
  rdfs:range xsd:string .

:targetEntityType rdf:type owl:DatatypeProperty ;
  rdfs:domain :AIRecommendation ;
  rdfs:range xsd:string .

:targetEntityId rdf:type owl:DatatypeProperty ;
  rdfs:domain :AIRecommendation ;
  rdfs:range xsd:string .

:confidenceScore rdf:type owl:DatatypeProperty ;
  rdfs:domain :AIRecommendation ;
  rdfs:range xsd:decimal .

:reasoning rdf:type owl:DatatypeProperty ;
  rdfs:domain :AIRecommendation ;
  rdfs:range xsd:string .

:suggestedAction rdf:type owl:DatatypeProperty ;
  rdfs:domain :AIRecommendation ;
  rdfs:range xsd:string .

:userAction rdf:type owl:DatatypeProperty ;
  rdfs:domain :AIRecommendation ;
  rdfs:range xsd:string .

:userActionTimestamp rdf:type owl:DatatypeProperty ;
  rdfs:domain :AIRecommendation ;
  rdfs:range xsd:dateTime .

:rejectionReason rdf:type owl:DatatypeProperty ;
  rdfs:domain :AIRecommendation ;
  rdfs:range xsd:string .

:feedbackText rdf:type owl:DatatypeProperty ;
  rdfs:domain :AIRecommendation ;
  rdfs:range xsd:string .

:communicationChannel rdf:type owl:DatatypeProperty ;
  rdfs:domain :Communication ;
  rdfs:range xsd:string .

:sentiment rdf:type owl:DatatypeProperty ;
  rdfs:domain :Communication ;
  rdfs:range xsd:string .

:responseTimeHours rdf:type owl:DatatypeProperty ;
  rdfs:domain :Communication ;
  rdfs:range xsd:decimal .

:taskStatus rdf:type owl:DatatypeProperty ;
  rdfs:domain :Task ;
  rdfs:range xsd:string .

:taskDueDate rdf:type owl:DatatypeProperty ;
  rdfs:domain :Task ;
  rdfs:range xsd:dateTime .

:metricTotalStagesPassed rdf:type owl:DatatypeProperty ;
  rdfs:domain :ApplicationMetrics ;
  rdfs:range xsd:int .

:metricTotalDurationDays rdf:type owl:DatatypeProperty ;
  rdfs:domain :ApplicationMetrics ;
  rdfs:range xsd:decimal .

:metricAverageEvaluationScore rdf:type owl:DatatypeProperty ;
  rdfs:domain :ApplicationMetrics ;
  rdfs:range xsd:decimal .

:metricCommunicationResponsiveness rdf:type owl:DatatypeProperty ;
  rdfs:domain :ApplicationMetrics ;
  rdfs:range xsd:decimal .
